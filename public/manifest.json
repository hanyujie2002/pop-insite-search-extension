{
  "manifest_version": 3,
  "name": "Google Search Popup",
  "description": "Shows a small window with Google search results.",
  "version": "1.0",
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "commands": {
    "open-search-popup": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Open search popup"
    }
  }
}